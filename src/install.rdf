	<em:name>Stratum</em:name>
	<em:version>dev-build</em:version>
	<em:description>Features old-style squared tabs, toolbar buttons and icons with blue glow.</em:description>
	<em:creator>FranklinDM</em:creator>
	<em:contributor>Mozilla Contributors</em:contributor>
	<em:homepageURL>https://franklindm.github.io/</em:homepageURL>

	<!-- Allow lightweight themes / personas to apply to this theme, using 'skinnable' -->
	<em:skinnable>true</em:skinnable>
	
	<!-- internalName must match the 'skinname' part of the relevant chrome.manifest directives in order for those mappings to be associated with this skin. -->
	<em:internalName>stratum</em:internalName>
  </Description>
</RDF>
